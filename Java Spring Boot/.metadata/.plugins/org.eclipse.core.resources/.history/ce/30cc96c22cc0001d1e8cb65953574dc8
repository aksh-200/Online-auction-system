package com.example.demo.entities;

public class Forget 
{
	String username ,answer;
	int q_id;
	
	
	public Forget() {
		super();
		// TODO Auto-generated constructor stub
	}


	public Forget(String username, String answer, int q_id) {
		super();
		this.username = username;
		this.answer = answer;
		this.q_id = q_id;
	}


	public String getUsername() {
		return username;
	}


	public void serUsername(String username) {
		this.username = username;
	}


	public String getAnswer() {
		return answer;
	}


	public void setAnswer(String answer) {
		this.answer = answer;
	}


	public int getQ_id() {
		return q_id;
	}


	public void setQ_id(int q_id) {
		this.q_id = q_id;
	}


	@Override
	public String toString() {
		return "Forget [username=" + username + ", answer=" + answer + ", q_id=" + q_id + "]";
	}
	
	
	
	
	

}
